<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="510.0" prefWidth="861.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="core.ui.UIController">
    <children>
        <GridPane layoutX="-2.0" layoutY="100.0" prefHeight="398.0" prefWidth="861.0">
            <children>
                <TableView fx:id="pullRequestTableView" prefHeight="411.0" prefWidth="772.0" GridPane.hgrow="ALWAYS"
                           GridPane.vgrow="ALWAYS">
                    <columns>
                        <TableColumn fx:id="pullRequestNrCol" prefWidth="88.0" text="number"/>
                        <TableColumn fx:id="pullRequestTitleCol" prefWidth="772.0" text="title"/>
                    </columns>
                </TableView>
            </children>
            <columnConstraints>
                <ColumnConstraints/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints vgrow="ALWAYS"/>
            </rowConstraints>
        </GridPane>
        <Button fx:id="refreshBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onRefreshBtnClicked"
                text="refresh"/>
        <TextField fx:id="filterStringInput" layoutX="14.0" layoutY="62.0" promptText="filter string"/>
        <Button fx:id="applyFilterBtn" layoutX="206.0" layoutY="62.0" mnemonicParsing="false"
                onAction="#onApplyFilterBtnClicked" text="apply filter"/>
        <Button fx:id="removeFilterBtn" layoutX="326.0" layoutY="62.0" mnemonicParsing="false"
                onAction="#onRemoveFilterBtnClicked" text="remove filter"/>
    </children>
</AnchorPane>
